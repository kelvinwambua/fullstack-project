{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    },
    "allowSyntheticDefaultImports": true
  },
  "resolutions": {
  "@urql/exchange-graphcache": "^7.0.0"
},
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "src/generated/graphql.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}